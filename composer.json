{
    "name": "acodingpeter/laravel-zip",
    "description": "fork of zanysoft/laravel-zip",
    "license": "MIT",
    "homepage": "https://github.com/acodingpeter/laravel-zip",
    "authors": [
        {
            "name": "A Coding Peter",
            "email": "p.hoermeyer@a-coding-project.de",
            "homepage": "https://github.com/acodingpeter/laravel-zip"
        }
    ],
    "keywords": [
        "laravel-zip",
        "laravel",
        "laravel5",
        "zip",
        "unzip",
        "extract",
        "backup",
        "ZipArchive",
        "multiple",
        "merge"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ZanySoft\\Zip\\ZipServiceProvider"
            ],
            "aliases": {
                "Zip": "ZanySoft\\Zip\\ZipFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
           "ZanySoft\\Zip\\": "src"
         }
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "scrutinizer/ocular": "^1.0"
    }
}
